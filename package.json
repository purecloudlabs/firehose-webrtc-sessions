{
  "name": "purecloud-streaming-client-webrtc-sessions",
  "version": "7.2.5",
  "description": "WebRTC Extensions for the PureCloud Streaming Client SDK",
  "main": "dist/webrtc-sessions.js",
  "pre-push": [
    "test"
  ],
  "files": [
    "src",
    "dist/webrtc-sessions.js",
    "dist/webrtc-sessions.js.map"
  ],
  "scripts": {
    "build": "webpack && webpack --env.production",
    "unit-test": "NODE_ENV=test nyc --reporter=lcov --reporter=cobertura ava test/unit/*.js",
    "test": "semistandard && npm run unit-test && npm run report",
    "semistandard": "semistandard",
    "semistandard:fix": "semistandard --fix",
    "report": "nyc report && nyc check-coverage --lines 100 --functions 100 --branches 100 --statements 100",
    "watch-tests": "watch 'npm test' src test"
  },
  "semistandard": {
    "ignore": [
      "/.nyc_output",
      "/coverage",
      "/scripts",
      "/dist"
    ]
  },
  "babel": {
    "presets": [
      "env"
    ],
    "env": {
      "test": {
        "plugins": [
          "istanbul"
        ]
      }
    }
  },
  "ava": {
    "files": [],
    "concurrency": 5,
    "failFast": true,
    "failWithoutAssertions": false,
    "tap": false,
    "powerAssert": true,
    "require": [
      "babel-register",
      "./test/helpers/setup-browser-env.js"
    ]
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/purecloudlabs/purecloud-streaming-client-webrtc-sessions.git"
  },
  "keywords": [
    "jingle",
    "xmpp",
    "streaming",
    "websocket",
    "signaling",
    "webrtc"
  ],
  "author": "Xander Dumaine",
  "license": "MIT",
  "homepage": "https://github.com/purecloudlabs/purecloud-streaming-client-webrtc-sessions#readme",
  "dependencies": {
    "jingle-media-data-session-purecloud": "^3.0.0",
    "jingle-media-session-purecloud": "^3.3.3",
    "jingle-purecloud": "^4.0.0",
    "jingle-stanza": "^2.0.2",
    "limiter": "^1.1.2",
    "lru-cache": "^4.1.2",
    "stanza.io": "^9.1.0",
    "uuid": "^3.1.0",
    "wildemitter": "^1.2.0"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-istanbul": "^5.1.1",
    "babel-preset-env": "^1.6.1",
    "browser-env": "^3.1.0",
    "jxt": "^3.0.8",
    "jxt-xmpp": "^3.2.2",
    "jxt-xmpp-types": "^3.0.0",
    "nyc": "^13.0.0",
    "semistandard": "^13.0.1",
    "sinon": "^2.3.8",
    "watch": "^1.0.2",
    "webpack": "^4.16.2",
    "webpack-cli": "^3.1.0"
  }
}
