{
  "name": "purecloud-streaming-client-webrtc-sessions",
  "version": "8.0.0",
  "description": "WebRTC Extensions for the PureCloud Streaming Client SDK",
  "main": "dist/webrtc-sessions.js",
  "files": [
    "src",
    "dist/webrtc-sessions.js",
    "dist/webrtc-sessions.js.map"
  ],
  "scripts": {
    "build": "webpack && webpack --env.production",
    "test": "NODE_ENV=test npm run lint && jest --runInBand",
    "test:watch": "NODE_ENV=test jest --watch --collectCoverage=false --runInBand",
    "lint": "npm run semistandard && npm run tslint",
    "tslint": "tslint --project . --config tslint.json",
    "semistandard": "semistandard",
    "semistandard:fix": "semistandard --fix",
    "greenkeep": "npx npm-check --update"
  },
  "pre-push": [
    "test"
  ],
  "semistandard": {
    "ignore": [
      "/coverage",
      "/scripts",
      "/dist"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/purecloudlabs/purecloud-streaming-client-webrtc-sessions.git"
  },
  "keywords": [
    "jingle",
    "xmpp",
    "streaming",
    "websocket",
    "signaling",
    "webrtc"
  ],
  "author": "Xander Dumaine",
  "license": "MIT",
  "homepage": "https://github.com/purecloudlabs/purecloud-streaming-client-webrtc-sessions#readme",
  "dependencies": {
    "jingle-media-data-session-purecloud": "^3.0.0",
    "jingle-media-session-purecloud": "^3.3.4",
    "jingle-purecloud": "^4.0.0",
    "jingle-stanza": "^2.0.2",
    "limiter": "^1.1.2",
    "lru-cache": "^5.1.1",
    "stanza": "^11.3.6",
    "uuid": "^3.1.0",
    "wildemitter": "^1.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/register": "^7.4.4",
    "@types/jest": "^24.0.15",
    "awesome-typescript-loader": "^5.2.1",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-istanbul": "^5.1.1",
    "babel-polyfill": "^6.26.0",
    "browser-env": "^3.2.6",
    "esm": "^3.2.25",
    "jest": "^24.8.0",
    "jxt": "^4.0.2",
    "jxt-xmpp": "^3.2.2",
    "jxt-xmpp-types": "^3.0.0",
    "semistandard": "^13.0.1",
    "sinon": "^7.3.2",
    "ts-jest": "^24.0.2",
    "tslint": "^5.18.0",
    "tslint-config-semistandard": "^8.0.1",
    "typescript": "^3.5.3",
    "watch": "^1.0.2",
    "webpack": "^4.35.3",
    "webpack-cli": "^3.3.5"
  }
}
